{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.10.0",
      "destination": "wwwroot/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/lib/bootbox.js/",
      "files": [
        "bootbox.min.js"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css"
      ]
    }
  ]
}